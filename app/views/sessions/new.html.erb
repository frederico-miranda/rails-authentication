<h2>Sign In</h2>
<% if flash[:errors] %>
<div class="errors">
<% flash[:errors].each do |error| %>
  <div class="error"><%= error %></div>
<% end %>
</div>
<% end %>
<%= form_with model: @user do |form| %>
<%= label_tag(:email, "Email: ") %> <%= form.text_field :email %>
<%= label_tag(:password, "Password: ") %> <%= form.password_field :password %>
<%= submit_tag("Sign In") %>
<% end %>
